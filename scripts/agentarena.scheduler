#!/usr/bin/env python3
"""
Script to start the Agent Arena server using uvicorn.
"""


import uvicorn

# Run the server
if __name__ == "__main__":
    print(f"Starting Agent Arena Scheduler")
    uvicorn.run(
        "agentarena.scheduler.app:app",
        host="0.0.0.0",
        port=8080,
        reload=True,
        workers=1,
    )
